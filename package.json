{
  "name": "starter-kit",
  "version": "1.0.0",
  "dependencies": {
    "imagesloaded": "^4.1.4",
    "sanitize.css": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chokidar-cli": "^1.2.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "csswring": "^7.0.0",
    "ejs": "^2.6.1",
    "ejs-cli": "^2.0.1",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "husky": "^1.1.2",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.5",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.1.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "license": "MIT",
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production run-s clean build:*",
    "build:ejs": "ejs-cli '**/!(_)*.ejs' -b src -o dist",
    "build:webpack": "webpack",
    "clean": "rimraf dist",
    "dev": "run-s clean watch",
    "format": "run-s format:*",
    "format:css": "stylelint 'src/**/*.css' --fix",
    "format:js": "eslint 'src/**/*.js' --fix",
    "start": "run-p watch:*",
    "watch:ejs": "chokidar 'src/**/*.ejs' -c 'npm run build:ejs' --initial",
    "watch:webpack": "webpack-dev-server --hot --inline"
  }
}
